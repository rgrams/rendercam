[project]
title = Rendercam
version = 2.0

[bootstrap]
main_collection = /main/main.collectionc
render = /rendercam/rendercam.renderc

[input]
game_binding = /input/game.input_bindingc

[rendercam]
debug_camera_name = 1

[display]
width = 1280
height = 600
vsync = 0

[physics]
scale = 0.02

[script]
shared_state = 1

[library]
include_dirs = rendercam

